<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressBar">
        <attr name="circleColor" format="color"/>
        <attr name="circleProgressColor" format="color"/>
        <attr name="circleWidth" format="dimension"></attr>
        <attr name="circleTextColor" format="color" />
        <attr name="circleTextSize" format="dimension" />
        <attr name="circleMax" format="integer"></attr>
        <attr name="circleTextIsDisplayable" format="boolean"></attr>
        <attr name="circleStyle">
            <enum name="circleSTROKE" value="0"></enum>
            <enum name="circleFILL" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>